                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <root>
                <ns:Users>
                    <ns:id>
                        <xsl:value-of select="'1'"/>
                    </ns:id>
                    <ns:firstName>
                        <xsl:value-of select="'Nelson'"/>
                    </ns:firstName>
                    <ns:lastName>
                        <xsl:value-of select="'Dias'"/>
                    </ns:lastName>
                    <ns:nationality>
