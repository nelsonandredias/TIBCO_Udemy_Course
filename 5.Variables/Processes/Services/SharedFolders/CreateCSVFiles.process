                <compressFile>None</compressFile>
                <append>true</append>
            </config>
            <pd:inputBindings>
                <pfx:WriteActivityInputTextClass>
                    <fileName>
                        <xsl:value-of select="'C:\Users\NelsonDias\OneDrive - P8G Group, Lda\Ambiente de Trabalho\TIBCO_Udemy_Course\Files\Input\users2.csv'"/>
